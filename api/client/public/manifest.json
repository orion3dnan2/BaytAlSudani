{
  "name": "البيت السوداني - السوق الإلكتروني الشامل",
  "short_name": "البيت السوداني",
  "description": "منصة البيت السوداني للمتاجر والخدمات - السوق الإلكتروني الشامل للسودان",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#2563eb",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "ar",
  "dir": "rtl",
  "categories": ["shopping", "business", "productivity"],
  "icons": [
    {
      "src": "/icons/icon-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-128x128.png",
      "sizes": "128x128",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-152x152.png",
      "sizes": "152x152",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "maskable any"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/desktop-home.png",
      "type": "image/png",
      "sizes": "1920x1080",
      "form_factor": "wide",
      "label": "الصفحة الرئيسية للبيت السوداني"
    },
    {
      "src": "/screenshots/mobile-marketplace.png",
      "type": "image/png",
      "sizes": "540x720",
      "form_factor": "narrow",
      "label": "السوق الإلكتروني على الهاتف المحمول"
    },
    {
      "src": "/screenshots/desktop-stores.png",
      "type": "image/png",
      "sizes": "1920x1080",
      "form_factor": "wide",
      "label": "صفحة المتاجر"
    },
    {
      "src": "/screenshots/mobile-products.png",
      "type": "image/png",
      "sizes": "540x720",
      "form_factor": "narrow",
      "label": "صفحة المنتجات على الهاتف"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "file_handlers": [
    {
      "action": "/import-data",
      "accept": {
        "text/csv": [".csv"],
        "application/pdf": [".pdf"],
        "application/vnd.ms-excel": [".xls", ".xlsx"],
        "application/json": [".json"]
      },
      "icons": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ],
      "launch_type": "single-client"
    }
  ],
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+sudanese-market",
      "url": "/handle-link?url=%s"
    },
    {
      "protocol": "web+bayt-sudani",
      "url": "/handle-protocol?action=%s"
    }
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "shortcuts": [
    {
      "name": "السوق",
      "short_name": "السوق",
      "description": "تصفح المنتجات في السوق",
      "url": "/marketplace",
      "icons": [
        {
          "src": "/icons/shortcut-marketplace.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "المتاجر",
      "short_name": "المتاجر",
      "description": "استكشف المتاجر المختلفة",
      "url": "/stores",
      "icons": [
        {
          "src": "/icons/shortcut-stores.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "الخدمات",
      "short_name": "الخدمات",
      "description": "تصفح الخدمات المتاحة",
      "url": "/services",
      "icons": [
        {
          "src": "/icons/shortcut-services.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "الوظائف",
      "short_name": "الوظائف",
      "description": "البحث عن فرص العمل",
      "url": "/jobs",
      "icons": [
        {
          "src": "/icons/shortcut-jobs.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui", "browser"],
  "launch_handler": {
    "client_mode": "navigate-existing"
  }
}